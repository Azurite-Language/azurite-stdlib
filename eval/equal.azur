(macro ("eval/equal" lang)(
    (translate ("eval" "=" "0" "0") ("any") (lang "" "true"))
    (translate ("eval" "=" "1" "1") ("any") (lang "" "true"))
    (translate ("eval" "=" "2" "2") ("any") (lang "" "true"))
    (translate ("eval" "=" "3" "3") ("any") (lang "" "true"))
    (translate ("eval" "=" "4" "4") ("any") (lang "" "true"))
    (translate ("eval" "=" "5" "5") ("any") (lang "" "true"))
    (translate ("eval" "=" "6" "6") ("any") (lang "" "true"))
    (translate ("eval" "=" "7" "7") ("any") (lang "" "true"))
    (translate ("eval" "=" "8" "8") ("any") (lang "" "true"))
    (translate ("eval" "=" "9" "9") ("any") (lang "" "true"))

    (translate ("eval" "=" "|x|0" "|y|0") ("any" "any") (lang "" "<eval (eval = {x} {y})>"))
    (translate ("eval" "=" "|x|1" "|y|1") ("any" "any") (lang "" "<eval (eval = {x} {y})>"))
    (translate ("eval" "=" "|x|2" "|y|2") ("any" "any") (lang "" "<eval (eval = {x} {y})>"))
    (translate ("eval" "=" "|x|3" "|y|3") ("any" "any") (lang "" "<eval (eval = {x} {y})>"))
    (translate ("eval" "=" "|x|4" "|y|4") ("any" "any") (lang "" "<eval (eval = {x} {y})>"))
    (translate ("eval" "=" "|x|5" "|y|5") ("any" "any") (lang "" "<eval (eval = {x} {y})>"))
    (translate ("eval" "=" "|x|6" "|y|6") ("any" "any") (lang "" "<eval (eval = {x} {y})>"))
    (translate ("eval" "=" "|x|7" "|y|7") ("any" "any") (lang "" "<eval (eval = {x} {y})>"))
    (translate ("eval" "=" "|x|8" "|y|8") ("any" "any") (lang "" "<eval (eval = {x} {y})>"))
    (translate ("eval" "=" "|x|9" "|y|9") ("any" "any") (lang "" "<eval (eval = {x} {y})>"))
    (translate ("eval" "=" 'a' 'b') ("any" "any") (lang "" "false"))
    (translate ("eval" "=" a b) ("any" "any") (lang "" "<eval (eval (= <eval (eval [a])> <eval (eval [b])>))>"))
))